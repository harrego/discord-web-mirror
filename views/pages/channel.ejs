  <style>
	 body {
		margin: 0;
		font-family: "Verdana", sans-serif;
		font-size: 12px;
	}
	.version {
		font-size: 10px;
		text-align: center;
		margin-top: 16px;
	}
	.content {
		margin: 0 auto;
		max-width: 600px;
	}

	.p-margin-0 p {
		margin: 0;
	}

	.message {
		background-color: #f5f5f5;
	}

	.message-header {
		display: flex;
		flex-direction: row;
		justify-content: space-between;
		background-color: darkgray;=
	}

	.message-header-channel {
		font-weight: bold;
	}

	.embed {
		max-width: 350px;
		margin-bottom: 16px;
		background-color: #f0f0f0;
	}

	.embed-header-container {
		background-color: #d1d1d1;
	}

	.embed-footer {
		background-color: #d1d1d1;
	}

	.embed-header-title {
		font-weight: bold;
	}
  </style>
</head>
<body>
	<div class="content">
